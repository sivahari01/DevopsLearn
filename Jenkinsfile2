pipeline {
    agent any
    environment {
        AWS_REGION = 'us-east-1'  // Set your preferred AWS region
    }
    stages {
        stage('Checkout') {
            steps {
               sh '''
                    ls
                    pwd
                    cd ..
                    cd Terraform 
                    terraform destroy -auto-approve \
                      -var="AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}" \
                      -var="AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}"
                '''
                 
            }
        }
    } // Close stages block
} // Close pipeline block

